{
  "expo": {
    "name": "The Trivialer",
    "slug": "the-trivialer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "the-trivialer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Test your knowledge with fun trivia questions across multiple categories and difficulty levels.",
    "privacy": "public",
    "privacyPolicyUrl": "https://mauriciollanos07.github.io/TrivialerPrivacyPolicy/",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maurostudio.thetrivialer",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maurostudio.thetrivialer",
      "versionCode": 2,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "blockedPermissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "proguardVersion": "6.2.2",
      "label": "The Trivialer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9d36a12b-4072-484d-a6cf-fcb3fd8f4f1e"
      }
    }
  }
}
